<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appprojet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.Light">

        <activity
            android:name=".ui.homepage.HomePageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.event_view.EventViewActivity" />

        <activity android:name=".ui.authentication.AuthenticationActivity" />
        <activity android:name=".ui.event_creation.EventCreationActivity" />
        <activity android:name=".ui.document_view.DocumentViewActivity" />
        <activity android:name=".ui.document_creation.DocumentCreationActivity" />

    </application>

</manifest>