<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DateFormLayout">
        <attr name="text" format="string" />
    </declare-styleable>

    <declare-styleable name="TextFormLayout">
        <attr name="hint" format="string" />
        <attr name="inputType" format="enum">
            <enum name="text" value="0"/>
            <enum name="password" value="1"/>
            <enum name="email" value="2"/>
            <enum name="singleLine" value="3"/>
        </attr>
        <attr name="colorTint" format="color" />
    </declare-styleable>
</resources>